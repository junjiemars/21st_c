
flag=prep_cppflags value="$NM_INCS" . ${NM_HOME}auto/cflags

case $CC_NAME in
	gcc) 
		flag=prep_cflags value="-std=c99" . ${NM_HOME}auto/cflags
		;;
	clang)
		flag=prep_cppflags value="$NM_INCS" . ${NM_HOME}auto/cflags
		;;
	msvc)
		flag=prep_lnkflags value="$CFLAGS" . ${NM_HOME}auto/cflags
		;;
esac	
