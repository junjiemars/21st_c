
nm_feature="endian"
nm_feature_name="nm_have_little_endian"
nm_feature_run=value
nm_feature_incs="#include <stdio.h>"
nm_feature_path=
nm_feature_libs=
nm_feature_test='int i=0x11223344;
                 char *p = (char *)&i;
            		 int le = (0x44 == *p);
                 printf("%d", le);'
. ${NM_HOME}auto/feature


nm_include="stdint.h" . ${NM_HOME}auto/include


nm_include="inttypes.h" . ${NM_HOME}auto/include


nm_include="stdbool.h" . ${NM_HOME}auto/include


nm_feature="sizeof(void*)"
nm_feature_name="nm_ptr_len"
nm_feature_run=value
nm_feature_incs="#include <stdio.h>"
nm_feature_path=
nm_feature_libs=
nm_feature_test='void* p; printf("%zu", sizeof p);'
. ${NM_HOME}auto/feature


nm_feature="sizeof(size_t)"
nm_feature_name="nm_size_t_len"
nm_feature_run=value
nm_feature_incs="#include <stdio.h>"
nm_feature_path=
nm_feature_libs=
nm_feature_test='printf("%zu", sizeof(size_t)*8);'
. ${NM_HOME}auto/feature



